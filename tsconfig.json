{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "types": ["vite/client"],
        "paths": {
            "@components/*": ["src/components/*"],
            "@static/*": ["src/static/*"],
            "@playground/*": ["src/playground/*"],
            "@utils/*": ["src/utils/*"],
            "@data": ["src/data.js"],
            "@helpers": ["src/helpers.js"]
        }
    }
}
